/**index.wxss**/

#findImg image {
  width: 100%;
  height: 100%;
  vertical-align: bottom;
}

#findBtn {
  position: absolute;
  bottom: 150px;
  width: 50%;
  margin: auto;
  right: 0;
  left: 0;
}

#findBtn button {
  background-color: #3598db;
  color: #fff;
}

#findFoot {
  position: absolute;
  bottom: 20px;
  right: 0;
  left: 0;
  margin: auto;
  color: #fff;
  font-size:12px;
}

#findFoot p {
  display: block;
  text-align: center;
}
